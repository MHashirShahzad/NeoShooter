; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NeoShooter"
config/description="Space shooter revamped.s"
run/main_scene="res://levels/scenes/start_animation.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/NeoShooterLogo.svg"

[audio]

buses/default_bus_layout="res://custom-stuff/default_bus_layout.tres"

[autoload]

BulletManager="*res://autoloads/bullet_manager.gd"
VFXManager="*res://autoloads/hit-effect/vfx_manager.tscn"
UIManager="*res://autoloads/ui/ui_manager.tscn"
SFXManager="*res://autoloads/audio/sfx_manager.tscn"
TransitionManager="*res://autoloads/transition/transition_manager.tscn"
GameManager="*res://autoloads/game_manager.gd"

[display]

window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/PaletteTools/plugin.cfg", "res://addons/TextureRectRounded/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[global_group]

Player="Group containing all player"
Camera="The custom space shooter camera"
Bullet=""
MainMenu=""
StageHazard=""

[gui]

theme/custom="res://custom-stuff/default_theme.tres"
theme/custom_font="res://assets/font/Sono/Sono-VariableFont_MONO,wght.ttf"

